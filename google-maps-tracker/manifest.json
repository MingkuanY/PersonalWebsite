{
  "manifest_version": 3,
  "name": "GeoSurfer",
  "description": "Tracking how much time you spend surfing Google Maps",
  "version": "1.0",
  "permissions": [
    "tabs", "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "/icons/icon16.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/icons/icon16.png",
      "48": "/icons/icon48.png",
      "128": "/icons/icon128.png"
    }
  }
}